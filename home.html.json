ont-size: 18px;\n    text-align: center;\n    font-style: normal;\n    width: 99.99%;\n    border: none;\n    margin-bottom: 10px;\n    margin-left: 15px;\n    /*margin-right: 15px;*/\n    /*position: relative;*/\n    /*top: 0%;*/\n    /*bottom: 0%;*/\n    /*right: 0%;*/\n    /*left: 5%;*/\n  }\n\n  fieldset {\n    margin-bottom: 10px;\n    border: none;\n  }\n\n  .hidden {\n    display: none;\n  }\n\n  /*@media screen and (min-width: 480px) {*/\n  /*  form {*/\n  /*    max-width: 480px;*/\n  /*    border: none;*/\n  /*  }*/\n  }\n</style>\n\n<div class=\"card\">\n  <form id=\"waitlist-form\" class=\"launchlist-form-popup\" action=\"https://getlaunchlist.com/s/JZIFPd\" method=\"post\" target=\"_blank\">\n    <fieldset>\n      <input type=\"email\" id=\"mail\" name=\"email\" placeholder=\"Enter your email\" required>\n    </fieldset>\n    <button type=\"submit\">Join the waitlist</button>\n  </form>\n  <p class=\"launchlist-form-popup-error hidden\"></p>\n</div>\n\n\n<script>\n  let form = document.querySelector(\".launchlist-form-popup\");\n  form.addEventListener(\"submit\", function(e) {\n    e.preventDefault();\n    let requiredFields = form.querySelectorAll(\"[required]\");\n    let isValid = true;\n    requiredFields.forEach((field) => {\n        if (field.value === \"\") {\n          isValid = false;\n          console.log(\"error\")\n          field.classList.add(\"error\");\n        } else {\n          console.log(\"ok\")\n          field.classList.remove(\"error\");\n        }\n        // submit form\n        if (isValid) {\n          let errorEl = document.querySelector(\".launchlist-form-popup-error\");\n          // Get the action url of the form\n          let action = form.getAttribute(\"action\");\n          // Get the form data\n          let formData = new FormData(form);\n          fetch(action, {\n              method: \"POST\",\n              body: formData,\n              headers: {\n                Accept: \"application/json\"\n              }\n            })\n            .then(response => response.json())\n            .then((data) => {\n              console.log(data);\n              if (data.ok) {\n                // replace iframe src\n                let iframe = document.querySelector(\".launchlist-iframe\");\n                iframe.src = data.embeddedLink;\n                toggleModal();\n              } else {\n                errorEl.innerText = data.error;\n                errorEl.classList.remove(\"hidden\");\n              }\n            })\n            .catch((error) => {\n              errorEl.classList.remove(\"hidden\");\n              errorEl.innerText = \"Something went wrong.\";\n            });\n        }\n    });\n  });\n</script>"},{"id":"silex-id-1671019520296-2","tagName":"DIV","type":"text-element","alt":null,"title":null,"isSectionContent":false,"visibility":{"desktop":true,"mobile":true},"style":{"mobile":{},"desktop":{"width":"100px","height":"140px","position":"absolute","top":"100px","left":"565px"}},"data":{"component":null},"children":[],"pageNames":[],"classList":[],"attr":{},"link":null,"enableEdit":true,"enableDrag":true,"enableDrop":false,"enableResize":{"top":true,"bottom":true,"left":true,"right":true},"selected":false,"useMinHeight":true,"innerHtml":"<p><b>NAMASTE CLOUD GAMING</b></p>"}]}